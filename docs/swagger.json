                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "structure"
                ],
                "summary": "lists available gateways",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/model.Gateway"
                            }
                        }
                    }
                }
            }
        },
        "/structure/tag/list/{gatewayid}": {
            "get": {
                "description": "lists tags",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "structure"
                ],
                "summary": "lists available tags - get the gateway_id from structure/gateway/list",
                "parameters": [
                    {
                        "type": "string",
                        "description": "id of the gateway",
                        "name": "gatewayid",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/model.Gateway"
                            }
                        }
                    }
                }
            }
